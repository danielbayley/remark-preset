    "remark-lint-code-block-style": "^1.0.0",
    "remark-lint-definition-case": "^1.0.0",
    "remark-lint-definition-spacing": "^1.0.0",
    "remark-lint-emphasis-marker": "^1.0.0",
    "remark-lint-fenced-code-flag": "^1.0.0",
